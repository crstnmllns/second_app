<!DOCTYPE html>
<html>
  <head>
    <title>SecondApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>


  </head>
  <body class="homepage is-preload" ">
    <div id="header">

      <!-- Nav -->
        <nav id="nav">
          <ul>
            <li><%= link_to 'index' , pages_index_path %></li>
            <li><%= link_to 'test' , pages_test_path %></li>
            <li><%= link_to 'login' , users_login_path %></li>
          </ul>
        </nav>
    </div>
    <%= yield %>
    <div id="footer">
      <div class="container">
        <div class="row">
          <div class="col-12">

            <!-- Copyright -->
              <div class="copyright">
                <ul class="menu">
                  <li>&copy; Untitled. All rights reserved.</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
                </ul>
              </div>

          </div>

        </div>
       </div>
      </div>

  </body>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</html>
